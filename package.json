{
  "name": "aframe-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for using A-Frame (VR) with React.",
  "dependencies": {
    "aframe": "^0.2.0",
    "aframe-htmltexture-component": "^0.2.1",
    "aframe-look-at-component": "^0.1.1",
    "aframe-react": "^1.2.0",
    "axios": "^0.13.1",
    "babel-polyfill": "^6.3.14",
    "justore": "^1.2.2",
    "react": "15.2.1",
    "react-dom": "15.2.1"
  },
  "scripts": {
    "build": "webpack",
    "preghpages": "npm run build && rm -rf gh-pages && cp -r src gh-pages && cp index.html build/bundle.js gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "gh-pages": "npm run ghpages",
    "server": "cd src && python -m SimpleHTTPServer 5555",
    "dev": "webpack-dev-server -d --content-base src --progress --watch --history-api-fallback --inline --hot --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngokevin/aframe-react-boilerplate.git"
  },
  "keywords": [
    "aframe",
    "react",
    "vr",
    "boilerplate",
    "a-frame",
    "mozvr",
    "webvr"
  ],
  "author": "Kevin Ngo <kngo@mozilla.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngokevin/aframe-react-boilerplate/issues"
  },
  "homepage": "https://github.com/ngokevin/aframe-react-boilerplate#readme",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "css-loader": "^0.23.0",
    "ghpages": "0.0.3",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
